<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>IRCCLIENT</RootNamespace>
    <TargetFramework>netcoreapp3.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\..\Comp\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="LogEngine">
      <HintPath>..\..\Comp\netcoreapp3.0\LogEngine.dll</HintPath>
    </Reference>
    <Reference Include="MWBot.net">
      <HintPath>..\..\Comp\netcoreapp3.0\MWBot.net.dll</HintPath>
    </Reference>
    <Reference Include="Utils">
      <HintPath>..\..\Comp\netcoreapp3.0\Utils.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Compile Update="IRCMessages.Designer.vb">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>IRCMessages.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="IRCMessages.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <CustomToolNamespace>My.Resources</CustomToolNamespace>
      <LastGenOutput>IRCMessages.Designer.vb</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
